	{
	case PETKAU_MACRO_Void:
		if (record->event.pressed)
			SEND_STRING(SS_TAP(X_V) PETKAU_DELAY SS_TAP(X_O) PETKAU_DELAY SS_TAP(X_I) PETKAU_DELAY SS_TAP(X_D));
		break;
	case PETKAU_MACRO_Break:
