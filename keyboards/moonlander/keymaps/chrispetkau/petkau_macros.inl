	case PETKAU_MACRO_NotEqual:
		if (record->event.pressed)
			SEND_STRING(SS_LSFT(SS_TAP(X_1)) PETKAU_DELAY SS_TAP(X_EQUAL));
		break;
	case PETKAU_MACRO_EqualsArrow:
		if (record->event.pressed)
			SEND_STRING(SS_TAP(X_EQUAL) PETKAU_DELAY SS_LSFT(SS_TAP(X_DOT)));
